{
    "name": "contass/e-cidade",
    "type": "project",
    "description": "E-cidade.",
    "keywords": ["e-cidade"],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-bcmath": "*",
        "ext-calendar": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pgsql": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "aferrandini/phpqrcode": "^1.0",
        "dbseller/legacy": "^1.0",
        "dbseller/sdk-api-pix-bb": "^2.0",
        "dex/composer-plug-and-play": "^0.13.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.5",
        "intervention/image": "^2.7",
        "jeremeamia/superclosure": "^2.0",
        "laminas/laminas-eventmanager": "2.2.10",
        "laravel/framework": "^8",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "lcobucci/jwt": "3.4.6",
        "league/fractal": "0.11.0",
        "michelf/php-markdown": "^1.6",
        "mormat/php-formula-interpreter": "1.0.0",
        "mpdf/mpdf": "v8.0.4",
        "ncjoes/office-converter": "^1.0",
        "nfephp-org/sped-common": "~5.0.40",
        "phpmailer/phpmailer": "^6.5",
        "phpoffice/phpspreadsheet": "^1.08",
        "phpoffice/phpword": "v0.17.*",
        "proner/phppimaco": "v1.4.5",
        "psr/log": "^1.1.0",
        "pusher/pusher-php-server": "~2.6",
        "robmorgan/phinx": "*",
        "symfony/console": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/polyfill-php56": "^1.1",
        "symfony/polyfill-php70": "^1.1",
        "symfony/process": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "files": [
            "definitions.php",
            "bootstrap/tenant_resolver.php",
            "std/autoload.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dex/composer-plug-and-play": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
