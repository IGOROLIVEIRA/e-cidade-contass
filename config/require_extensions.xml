<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuracoes>
  <modulos>
  	<modulo name="zip" valorpadrao="zip" label="Zip"/>
  	<modulo name="php-pgsql" valorpadrao="pgsql" label="PostgreSQL"/>
  	<modulo name="xmlwriter" valorpadrao="xmlwriter" label="XMLWriter"/>
  	<modulo name="libxml" valorpadrao="libxml" label="libxml"/>
  	<modulo name="php-dom" valorpadrao="dom" label="Document Object Model"/>
  	<modulo name="php-gd" valorpadrao="gd" label="Processamento de Imagem (GD)"/>
  	<modulo name="php-mhash" valorpadrao="mhash" label="Mhash"/>
  	<modulo name="php-mcrypt" valorpadrao="mcrypt" label="Mcrypt"/>
  	<modulo name="php-bcmath" valorpadrao="bcmath" label="BCMath Funcoes Matematicas de Precisao Arbitraria"/>
  	<modulo name="mbstring" valorpadrao="mbstring" label="Multibyte String"/>
  </modulos>

  <parametros>
    <parametro name="register_globals" bool="true" valorpadrao="1" operacao="==" label_valorpadrao="On" label="Register Globals"/>
    <parametro name="register_long_arrays" bool="true" valorpadrao="1" operacao="==" label_valorpadrao="On" label="Register Long Arrays"/>
    <parametro name="register_argc_argv" bool="true" valorpadrao="1" operacao="==" label_valorpadrao="On" label="Register Argc Argv"/>
    <parametro name="post_max_size" bool="false" valorpadrao="64M" operacao="&gt;=" label_valorpadrao="64M" label="Post Max Size"/>
    <parametro name="magic_quotes_gpc" bool="true" valorpadrao="1" operacao="==" label_valorpadrao="On" label="Magic Quotes Gpc"/>
    <parametro name="upload_max_filesize" bool="false" valorpadrao="64M" operacao="&gt;=" label_valorpadrao="64M" label="Upload Max Filesize"/>
    <parametro name="default_socket_timeout" bool="false" valorpadrao="60000" operacao="&gt;=" label_valorpadrao="60000" label="Default Socket Timeout"/>
    <parametro name="max_execution_time" bool="false" valorpadrao="60000" operacao="&gt;=" label_valorpadrao="60000" label="Max Execution Time"/>
    <parametro name="memory_limit" bool="false" valorpadrao="512M" operacao="&gt;=" label_valorpadrao="512M" label="Memory Limit"/>
    <parametro name="allow_call_time_pass_reference" bool="true" valorpadrao="1" operacao="==" label_valorpadrao="On" label="Allow Call Time Pass Reference"/>
    <parametro name="display_errors" bool="false" valorpadrao="0" operacao="==" label_valorpadrao="Off" label="Display Errors"/>
    <parametro name="session.gc_maxlifetime" bool="false" valorpadrao="7200" operacao="&gt;=" label_valorpadrao="7200" label="Session.gc Maxlifetime"/>
  </parametros>

  <database> 
  <parametro 
    name='server_version'               
    label='Server Version'              
    valorpadrao='9.2.*'   
    bool='true'   
    valor_min='90200' 
    valor_max='90299'> 
  </parametro>   
  <parametro 
    name='default_with_oids'            
    label='Default With Oids'           
    valorpadrao='on'      
    bool='false'  
    valor_min='null'  
    valor_max='null'> 
  </parametro> 
  <parametro 
    name='server_encoding'              
    label='Server Encoding'             
    valorpadrao='LATIN1'  
    bool='false'  
    valor_min='null'  
    valor_max='null'> 
  </parametro> 
  <parametro 
    name='bytea_output'                 
    label='Bytea Output'                
    valorpadrao='escape'  
    bool='false'  
    valor_min='null'  
    valor_max='null'> 
  </parametro> 
  <parametro 
    name='escape_string_warning'        
    label='Escape String Warning'       
    valorpadrao='off'     
    bool='false'  
    valor_min='null'  
    valor_max='null'> 
  </parametro> 
  <parametro 
    name='standard_conforming_strings'  
    label='Standard Conforming Strings' 
    valorpadrao='off'     
    bool='false'  
    valor_min='null'  
    valor_max='null'> 
  </parametro> 
</database>

  <browsers>
    <browser name="firefox" versao="1.5.*"/>
    <browser name="firefox" versao="2.0.*"/>
    <browser name="firefox" versao="3.0.*"/>
    <browser name="firefox" versao="3.1.*"/>
    <browser name="firefox" versao="3.5.*"/>
    <browser name="firefox" versao="3.6.*"/>
	<browser name="firefox" versao="5.0.*"/>
	<browser name="firefox" versao="5.0.0"/>
	<browser name="firefox" versao="5.0"/>
    <browser name="firefox" versao="11.0.*"/>
    <browser name="firefox" versao="12.0.*"/>
    <browser name="firefox" versao="15.0.1"/>
	<browser name="firefox" versao="16.0.*"/>
  </browsers>

  <diretorio name="dbportal2" valorpadrao="e-cidade" label="Diretorio padrao para o e-cidade"/>

  <template_login label="Diretorio padrao para templates de login" src="templates/template_login_e-cidade.php"/>
</configuracoes>
